                    <p class="small text-white-70 mb-0">
                    <%= post.currency %> <%= number_with_precision(post.price, precision: 0, :delimiter => ',') %> <i class="fas fa-map-marker ml-2"></i>
                    <%#= post.town.titleize if post.town %>, <%#= post.country_name if post.country  %> <%#= link_to (flag_icon(post.country.downcase.to_sym)), post_path(post.slug) if post.country %> </span>
                  </p>
                    <p class="small text-white-70 mb-0 wht">
                      by <%= link_to post.user.displayname.titleize, user_path(post.user.slug) if post.user.displayname %>
                    </p>
                  </div>
                </div>
              </div>
            </span>
          </div>
