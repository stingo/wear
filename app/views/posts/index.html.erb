 <% if current_user.is_a?(GuestUser) %>



    <!-- Hero Section -->
   
    <div class="d-md-flex bg-img-hero africanwear-mega-bg ">

      
      <div class="container d-md-flex align-items-md-center height-md-100vh position-relative space-top-2 space-bottom-3 space-top-md-3 space-top-lg-2 space-bottom-md-0">
        <div class="w-lg-65 mb-5">
          <div class="w-lg-65 w-xl-50 mb-5">
            <h1 class="display-4 text-white">Sell your <strong class="font-weight-semi-bold">house</strong> online</h1>
          </div>
 <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>







          <!-- Form -->
          <form class="js-validate">
            <div class="form-row mb-2">
              



             <div class="col-lg-6 col-md-12 col-xs-12 col-sm-12 mb-2">
                <div class="js-form-message">
                  <label class="sr-only">Displayname</label>
                  <div class="input-group">
                                 <small class="text-white"><%= f.text_field :displayname, required: true, :placeholder => 'enter your displayname', :class => "form-control" %>
                                 only letters(a-z), numbers(0-9), between 6-50 are allowed, no spaces between characters </small>
                  </div>
                </div>
              </div>

                            <div class="col-lg-6 col-md-12 col-xs-12 col-sm-12 mb-2">
                <div class="js-form-message">
                  <label class="sr-only" >Date of birth</label>
                  <div class="input-group">
                                <%= f.input :date_of_birth, as: :date, label: false, :order => [:day, :month, :year], :start_year => 2000,:end_year => 1918, required: true %>
              <small class="text-white">For analysis only and we will never share your date of birth with other users</small>
                  </div>
                </div>
              </div>





              <div class="col-lg-6 col-md-12 col-xs-12 col-sm-12 mb-2">
                <div class="js-form-message">
                  <label class="sr-only" >Email</label>
                  <div class="input-group">
                                <%= f.email_field :email, required: true, :pattern => '[^@]+@[^@]+\.[a-zA-Z]{2,6}', :class => "form-control", :placeholder => "Your Email" %>
                  </div>
                </div>
              </div>



              <div class="col-lg-6 col-md-12 col-xs-12 col-sm-12 mb-2">
                <div class="js-form-message">
                  <label class="sr-only">Password</label>
                  <div class="input-group">
                                 <%= f.password_field :password, required: true, autocomplete: "off", :class => "form-control", :placeholder => "Password"  %>
                                 

                  </div>
                </div>
              </div>










 


       

            <!-- Checkbox -->
            <div class="js-form-message">
              <div class="custom-control custom-checkbox d-flex align-items-center text-muted">



                  <span class="help-block"><strong><%= f.check_box :agreed_with_terms, required: true, :class => "text-info-strong" %>&nbsp <small class="text-white">I have read and agree to the
    <a href="https://www.upfrica.com/guides/what-is-not-allowed-on-upfrica" target="_blank" class="link-white">Terms of use</a>

    <span class="text-white"> of Africanwear.io</span></a>
                                    </strong></small>
                </label>
              </div>
            </div>
            <!-- End Checkbox -->



          
              <div class="col-lg-6 col-md-12 col-xs-12 col-sm-12 mb-2 mt-2">
                <div class="js-form-message">

                <%= f.submit "Sign Up",  :class => "btn btn-block btn-primary transition-3d-hover" %>

                
              </div>
            </div>



</div>
          </form>
          <!-- End Form -->
        </div>
<% end %>
